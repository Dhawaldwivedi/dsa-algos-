// linear search implementation
#include <iostream>
using namespace std ;

int main()
{
    int n ;
    int i;
    int a[n];
    cout<<"enter the  number of elements\n";
    cin>>n ;
    cout<<"enter the elements of array\n";
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    int num ;
    for(i=0;i<n;i++)
    {
        if(num==a[i])
        cout<<"number found\n ";
        else 
        cout<<"number not found"<<endl ;
    }
    return 0 ;
}
